#include <stdio.h>

int main() {
    int n;
    printf("Enter size: ");
    scanf("%d", &n);
    int nums[n];

    printf("Enter array: ");
    for (int i = 0; i < n; i++) scanf("%d", &nums[i]);

    int answer[n];
    for (int i = 0; i < n; i++) {
        int prod = 1;
        for (int j = 0; j < n; j++) {
            if (i != j)
                prod *= nums[j];
        }
        answer[i] = prod;
    }

    printf("Result: ");
    for (int i = 0; i < n; i++) printf("%d ", answer[i]);
    return 0;
}